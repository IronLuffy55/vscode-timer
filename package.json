{
  "name": "ie55-timer",
  "displayName": "ie55-timer",
  "description": "timer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "TypeScript",
      "properties": {
        "ie55timer.duration": {
          "type": "number",
          "default": 30,
          "description": "Default time for starting a timer"
        }
      }
    },
    "commands": [
      {
        "command": "ie55-timer.startTimer",
        "title": "Start Timer"
      },
      {
        "command": "ie55-timer.pauseTimer",
        "title": "Pause Timer"
      },
      {
        "command": "ie55-timer.stopTimer",
        "title": "Stop Timer"
      },
      {
        "command": "ie55-timer.restartTimer",
        "title": "Restart Timer"
      },
      {
        "command": "ie55-timer.showTimer",
        "title": "Show Timer"
      },
      {
        "command": "ie55-timer.hideTimer",
        "title": "Hide Timer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
